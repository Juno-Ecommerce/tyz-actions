{
  "version": 2,
  "builds": [{ "src": "server.ts", "use": "@vercel/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/server.ts" }],
  "functions": {
    "server.ts": {
      "outputFileTracingIncludes": {
        "node_modules/lighthouse/**": "*",
        "node_modules/chrome-launcher/**": "*"
      }
    }
  }
}